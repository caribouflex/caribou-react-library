{
  "name": "react-component-library",
  "version": "1.0.0",
  "description": "react-component-library React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
