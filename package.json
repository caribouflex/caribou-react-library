    "react-dom": "^16.4.2",
    "react-styleguidist": "^7.2.3",
    "styled-components": "^3.4.2",
    "webpack": "^4.16.5",
    "webpack-blocks": "^1.0.0"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
